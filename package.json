    "koa-validate": "^1.0.7",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "mongoose": "^5.0.14"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
